   "source": [
    "from threading import Thread\n",
    "\n",
    "\n",
    "def async(f):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        thr = Thread(target=f, args=args, kwargs=kwargs)\n",
    "        thr.start()\n",
    "    return wrapper"
   ]
  }
